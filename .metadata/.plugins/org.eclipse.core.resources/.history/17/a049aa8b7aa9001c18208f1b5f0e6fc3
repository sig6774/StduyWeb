package Day6;

class 게시물{
	int 번호;
	String 제목;
	String 내용;
	String 작성일;
}

class 게시물저장소{
	게시물[] 모든게시물;
	게시물저장소() {
		모든게시물 = new 게시물[3];
		
		모든게시물[0] = new 게시물();
		모든게시물[0].번호 = 3;
		모든게시물[0].제목 = "한식";
		모든게시물[0].내용= "비빔밥";
		
		모든게시물[1] = new 게시물();
		모든게시물[1].번호 = 2;
		모든게시물[1].제목 = "중식";
		모든게시물[1].내용= "볶음밥";
		
		모든게시물[2] = new 게시물();
		모든게시물[2].번호 = 1;
		모든게시물[2].제목 = "일식";
		모든게시물[2].내용= "초밥";
		
		
	}
}
public class 게시판 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
